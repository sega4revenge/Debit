  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'AlamofireObjectMapper'
  pod 'Highlighter'	
 pod 'FSCalendar'

end
